{
  "project": "{{ project_name }}",
  "version": "1.0.0",
  "created": "{{ created_date }}",
  "domains": {{ domains | tojson }},
  "expertise_threshold": {
    "confidence": 0.8,
    "usage_count": 10
  },
  "auto_improve": true,
  "skill_generation": true,
  "agents": {
    "build-agent": {
      "model": "sonnet",
      "max_concurrent": 4,
      "timeout_minutes": 30
    },
    "scout-agent": {
      "model": "haiku",
      "max_concurrent": 8,
      "timeout_minutes": 5
    },
    "reviewer-agent": {
      "model": "sonnet",
      "max_concurrent": 2,
      "timeout_minutes": 15
    }
  },
  "parallel_execution": {
    "max_worktrees": 4,
    "merge_strategy": "no-ff",
    "auto_cleanup": true,
    "test_before_merge": true
  },
  "output_directories": {
    "specs": "specs/",
    "reviews": "reviews/",
    "fix_reports": "fix-reports/",
    "build_reports": "build-reports/"
  },
  "hooks": {
    "PreToolUse": [],
    "PostToolUse": [],
    "Stop": [],
    "SubagentStop": []
  }
}
